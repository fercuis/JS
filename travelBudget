let diasDeViaje = 15
let presupuesto = 2000
let comida
let CantidadComidas = 30
let budgetComidas = presupuesto / CantidadComidas
budgetComidas = Math.round(budgetComidas)
alert("Puedes gastar " + "$" + budgetComidas + " en cada comida para que te alcance la plata durante los " + diasDeViaje + " dias de viaje.");
